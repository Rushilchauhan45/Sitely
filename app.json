{
  "expo": {
    "name": "SiteLy",
    "slug": "sitely-app-development",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/android-icon-foreground.png",
    "scheme": "siteexpense",
    "owner": "technorush_05",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0EA5E9"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.siteexpensetracker",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.siteexpensetracker",
      "adaptiveIcon": {
        "backgroundColor": "#0EA5E9",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-sqlite",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow SiteLy to access your photos to set profile picture."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f416291c-2278-41fc-81ec-c7b71a67a3c1"
      }
    }
  }
}
